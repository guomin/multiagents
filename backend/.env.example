# Tavily AI搜索服务
# 获取方式：访问 https://tavily.com/ 注册（免费1000次/月）
TAVILY_API_KEY=your_tavily_api_key_here

# DeepSeek配置
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# ZhipuAI配置 (智谱AI)
# 获取方式：访问 https://open.bigmodel.cn/ 注册并创建API Key
ZHIPUAI_API_KEY=your_zhipuai_api_key_here
ZHIPUAI_BASE_URL=https://open.bigmodel.cn/api/paas/v4

# OpenAI配置 (可选)
OPENAI_API_KEY=your_openai_api_key_here

# Model Provider Configuration
# 可选值: openai, deepseek, zhipuai
MODEL_PROVIDER=deepseek
DEFAULT_MODEL=deepseek-chat
TEMPERATURE=0.7

# Agent-Specific Model Configuration (智能体专属配置)
# 可选: 为特定智能体指定不同的模型
# 优先级高于全局配置
#
# 示例 1: 预算控制智能体使用智谱AI GLM-4.7，其他智能体使用DeepSeek
# BUDGET_CONTROLLER_PROVIDER=zhipuai
# BUDGET_CONTROLLER_MODEL=glm-4.7
#
# 示例 2: 所有智能体都使用智谱AI
# MODEL_PROVIDER=zhipuai
# DEFAULT_MODEL=glm-4.7
#
# 支持的智能体类型:
# - CURATOR (策划智能体)
# - SPATIAL_DESIGNER (空间设计智能体)
# - VISUAL_DESIGNER (视觉设计智能体)
# - INTERACTIVE_TECH (互动技术智能体)
# - BUDGET_CONTROLLER (预算控制智能体)
# - SUPERVISOR (监督智能体)
#
# 配置示例:
# CURATOR_PROVIDER=openai
# CURATOR_MODEL=gpt-4-turbo-preview
# SPATIAL_DESIGNER_PROVIDER=deepseek
# VISUAL_DESIGNER_PROVIDER=zhipuai
# VISUAL_DESIGNER_MODEL=glm-4-flash
# INTERACTIVE_TECH_PROVIDER=zhipuai
# INTERACTIVE_TECH_MODEL=glm-4.7
# SUPERVISOR_PROVIDER=openai
# SUPERVISOR_MODEL=gpt-4

# Server Configuration
PORT=3001
NODE_ENV=development
